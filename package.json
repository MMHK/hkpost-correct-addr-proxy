{
  "name": "hkpost-correct-addr-proxy",
  "version": "1.0.0",
  "description": "Proxy for HongKong Post Correct Addressing Services",
  "main": "index.js",
  "author": "sam@mixmedia.com",
  "license": "Apache-2.0",
  "scripts": {
    "dev": "nodemon",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.cjs",
    "start": "node dist/bundle.js",
    "test": "cross-env NODE_OPTIONS='--experimental-vm-modules' jest --testNamePattern='hkpost'",
    "test:street": "cross-env NODE_OPTIONS='--experimental-vm-modules' jest --testNamePattern='street'",
    "test:building": "cross-env NODE_OPTIONS='--experimental-vm-modules' jest --testNamePattern='building'",
    "test:gov": "cross-env NODE_OPTIONS='--experimental-vm-modules' jest --testNamePattern='data-gov-hk'",
    "test:multiple": "cross-env NODE_OPTIONS='--experimental-vm-modules --trace-warnings' jest --testNamePattern='multiple-records'"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.25.2",
    "@babel/node": "^7.25.0",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@types/node": "^22.4.0",
    "autoprefixer": "latest",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "csv-parser": "^3.0.0",
    "dify-client": "^2.3.2",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.0",
    "inter-ui": "^4.0.2",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "postcss": "latest",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^10.0.2",
    "style-loader": "^4.0.0",
    "url-loader": "^4.1.1",
    "uuid": "^11.0.3",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@fastify/one-line-logger": "^2.0.0",
    "@fastify/static": "^8.0.2",
    "@fastify/swagger": "^9.2.0",
    "@fastify/swagger-ui": "^5.1.0",
    "axios": "^1.7.8",
    "cheerio": "^1.0.0",
    "dotenv": "^16.4.5",
    "fastify": "^5.1.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
